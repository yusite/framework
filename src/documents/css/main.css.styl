.yzj-header
	height 35em
	padding-top 10em
	margin-bottom 2em
	background-image url(../img/headerbg.jpg)
	background-size cover
	color #fff
	text-align center
	h1
		font-size 4em
		margin-bottom 1em
	a
		color white

.navbar-default
	position absolute
	background-color transparent

.navbar-default .navbar-nav
	li a
		&:hover
			background-color transparent
	li.active a
		background-color transparent
		&:hover
			background-color transparent
